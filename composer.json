{
    "name": "cleaniquecoders/artisan-makers",
    "description": "Built with Laravel Standalone Package Creator",
    "license": "MIT",
    "authors": [
        {
            "name": "Nasrul Hazim",
            "email": "nasrulhazim.m@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CleaniqueCoders\\ArtisanMakers\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "CleaniqueCoders\\ArtisanMakers\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1",
        "illuminate/support": "^5.5|^5.6|^5.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5|^7.0",
        "orchestra/testbench": "3.5.*|3.6.*|3.7.*"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
